<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trial.safenote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SafeNote">
        <activity
            android:name=".TrashActivity"
            android:exported="true" />
        <activity
            android:name=".ProtectedNotesActivity"
            android:exported="true" />
        <activity
            android:name=".BaseActivity"
            android:exported="true" />
        <activity
            android:name=".ViewNoteActivity"
            android:exported="true"
            android:parentActivityName=".NotesActivity" />
        <activity
            android:name=".EditNoteActivity"
            android:exported="true"
            android:parentActivityName=".NotesActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateNoteActivity"
            android:exported="true"
            android:parentActivityName=".NotesActivity" />
        <activity
            android:name=".NotesActivity"
            android:exported="true" />
    </application>

</manifest>